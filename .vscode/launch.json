    "version": "0.2.0",
    "configurations": [
        {
            "request": "launch",
            "name": "Deno",
            "type": "pwa-node",
            "program": "${workspaceFolder}/server.jsx",
            "cwd": "${workspaceFolder}",
            "runtimeExecutable": "deno",
            "runtimeArgs": [
                "run",
                "--unstable",
                "--import-map",
                "deno.importmap",
                "--no-check",
                "--inspect",
                "--allow-all"
            ],
            "attachSimplePort": 9229
        }, {
            "request": "launch",
            "name": "Deploy",
            "type": "pwa-node",
            "program": "${workspaceFolder}/server.js",
            "cwd": "${workspaceFolder}",
            "runtimeExecutable": "deno",
            "runtimeArgs": [
                "run",
                "--unstable",
                "--no-check",
                "--inspect",
                "--allow-all"
            ],
            "attachSimplePort": 9229
        }
    ]
}